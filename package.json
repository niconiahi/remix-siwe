{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production netlify dev",
    "typecheck": "tsc -b",
    "build": "run-s \"build:*\"",
    "build:css": "npm run gen:css -- --minify",
    "build:remix": "remix build",
    "lint": "eslint . -c .eslintrc.js --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run gen:css -- --watch",
    "dev:remix": "remix dev",
    "gen:css": "postcss styles --base styles --dir app/styles"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@prisma/client": "^4.7.1",
    "@remix-run/netlify": "^1.8.2",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "siwe": "^1.1.6",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
